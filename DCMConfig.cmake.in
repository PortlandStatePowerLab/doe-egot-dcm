@PACKAGE_INIT@
...
set_and_check(DCM_INCLUDE_DIR "@PACKAGE_INCLUDE_INSTALL_DIR@")
set_and_check(DCM_SYSCONFIG_DIR "@PACKAGE_SYSCONFIG_INSTALL_DIR@")
set(_supported_components ECS HTTPS)
check_required_components(DCM)

foreach(_comp ${DCM_FIND_COMPONENTS})
  if (NOT _comp IN_LIST _supported_components)
    set(DCM_FOUND False)
    set(DCM_NOT_FOUND_MESSAGE "Unsupported component: ${_comp}")
  endif()
  include("${CMAKE_CURRENT_LIST_DIR}/DCM${_comp}Targets.cmake")
endforeach()